{
	"editor.occurrencesHighlight": "off",
	"editor.selectionHighlight": false,
	"workbench.iconTheme": "symbols",
	"tabnine.experimentalAutoImports": true,
	"editor.fontFamily": "Jetbrains Mono", // Font Family
	"editor.fontLigatures": true, // Font ligatures
	"colorize.languages": ["typescript", "javascript", "css", "scss"],
	"editor.quickSuggestions": {
		"other": true,
		"comments": false,
		"strings": true
	},
	// Theme Setup.
	"editor.fontSize": 16.5, // valor monitor 16.5 - valor only notebook 15.5
	"editor.lineHeight": 24.65,
	"editor.letterSpacing": 0.4,
	"editor.fontWeight": "400",
	"editor.cursorStyle": "line",
	"editor.cursorBlinking": "blink",
	"editor.renderLineHighlight": "gutter",
	"editor.renderWhitespace": "all",
	"editor.snippetSuggestions": "top",
	"workbench.startupEditor": "newUntitledFile",
	"editor.glyphMargin": true,
	"workbench.editor.enablePreview": false,
	"explorer.confirmDragAndDrop": false,
	"files.trimTrailingWhitespace": true,
	"files.trimFinalNewlines": true,
	"drink.water.remindText": "Vamo beber caf√© caraio",
	// "typescript.inlayHints.parameterNames.enabled": "all",
	"editor.guides.bracketPairs": "active",
	"editor.bracketPairColorization.enabled": true,
	"editor.quickSuggestionsDelay": 0,
	"editor.linkedEditing": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"name": "No italics",
				"scope": [
					"comment",
					"markup.quote",
					"variable.language",
					"variable.parameter"
				],
				"settings": {
					"fontStyle": "italic"
				}
			}
		]
	},
	"git.autofetch": true,
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"workbench.quickOpen.preserveInput": true,
	"workbench.tree.indent": 15,
	"task.quickOpen.history": 10,
	"task.quickOpen.showAll": true,
	"task.quickOpen.skip": true,
	"editor.inlineSuggest.enabled": true,
	"github.copilot.enable": {
		"*": true,
		"plaintext": true,
		"markdown": true,
		"scminput": false,
		"yaml": false
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.cursorSmoothCaretAnimation": "off",
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"files.associations": {
		"*.tsx": "typescriptreact"
	},
	"typescript.updateImportsOnFileMove.enabled": "always",
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"css.hover.documentation": false,
	"scss.hover.documentation": false,
	"less.hover.documentation": false,
	"typescript.preferences.importModuleSpecifier": "non-relative",
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"workbench.list.horizontalScrolling": true,
	"workbench.list.automaticKeyboardNavigation": false,
	"workbench.list.smoothScrolling": true,
	"editor.smoothScrolling": true,
	"workbench.reduceMotion": "on",
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.stickyScroll.enabled": true,
	"explorer.compactFolders": false,
	"terminal.integrated.cursorBlinking": true,
	"material-icon-theme.activeIconPack": "nest",
	"[prisma]": {
		"editor.formatOnSave": true
	},
	"editor.accessibilitySupport": "off",
	// auto collapse minimap
	"editor.minimap.autohide": true,
	"terminal.integrated.env.windows": {},
	"terminal.integrated.fontFamily": "JetBrains Mono",
	"sqltools.useNodeRuntime": true,
	"workbench.colorTheme": "Horizon Bold",
	"powermode.enabled": false,
	"console-ninja.featureSet": "Community",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"gitlens.views.tags.branches.layout": "list",
	"symbols.hidesExplorerArrows": false,
	"terminal.integrated.env.linux": {},
	"explorer.confirmDelete": false,
	"window.menuBarVisibility": "compact",
	"window.commandCenter": false,
	"workbench.productIconTheme": "material-product-icons"
}
